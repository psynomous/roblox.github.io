function resendTrade(t)

{



    $.ajax({

        url: 'api/get-info-claim',

        type: 'POST',

        headers: {

            'X-CSRF-TOKEN': $('meta[name="csrf-token"]').attr('content')

        },

        data: {

            'tracking': t

        },

        dataType: 'json',

        success: function (data) {        

            Swal.fire({

                icon: 'info',

                title: 'Would you like to us to send the trade?',

                text: `We'll send the ${data.limited_name} back to ${data.username} (${data.userid}) that placed the order instantly.`,

                customClass: 'swal-wide',

                showDenyButton: true,

                confirmButtonText: `Yes`,

                denyButtonText: `No`,

            }).then((result) => {

                if (result.isConfirmed) {

                    $.ajax({

                        url: 'api/claim-trade',

                        type: 'POST',

                        headers: {

                            'X-CSRF-TOKEN': $('meta[name="csrf-token"]').attr('content')

                        },

                        data: {

                            'tracking': t

                        },

                        dataType: 'json',

                        success: function (data2) {        

                            Swal.fire({

                                icon: 'success',

                                title: 'Nice!',

                                text: data2.msg,

                                customClass: 'swal-wide',

                            })

                        },

                        error: function (data2){



                            console.log(data2)

                            Swal.fire({

                                icon: 'error',

                                title: 'Oops...',

                                text: data2.responseJSON.msg,

                                customClass: 'swal-wide',

                            })

                        }

                    });

                }

            });

        },

        error: function (data){



            console.log(data)

            Swal.fire({

                icon: 'error',

                title: 'Oops...',

                text: data.responseJSON.msg,

                customClass: 'swal-wide',

            })

        }

    });

}



function claimLookup()

{



    $('#contentClaim').html(`<div class="spinner-grow text-light mx-auto" role="status">

        <span class="visually-hidden">Loading...</span>

        </div>`)





    $.ajax({

        url: 'api/check-claims',

        type: 'POST',

        headers: {

            'X-CSRF-TOKEN': $('meta[name="csrf-token"]').attr('content')

        },

        data: {

            'username': $('#usernameInput').val()

        },

        dataType: 'json',

        success: function (data) {

            //console.log(data)

            

            $('#contentClaim').html(``)



            if(Object.values(data).length === 0){

                $('#contentClaim').html(`<p class="text-center text-light">Nothing Here...</p><br>`)

            } else {

                Object.values(data).forEach(element => {

                $('#contentClaim').append(`

                    <div class="col-md-3 mt-2">

                        <div class="card p-2 h-100" style="background: #181A20;border-radius: 10px;" onclick="resendTrade('${element.tracking}')">

                            <img src="https://images.rbxflip.com/asset-thumbnail/image?assetId=${element.limited_id}&width=420&height=420&format=png" style="width: 120px;" class="center mx-auto" alt="">

                            <p class="text-light text-center">${element.limited_name}</p>

                        </div>

                    </div>

                `)

            });

            }

            

        },

        error: function (data){



            console.log(data)

            Swal.fire({

            icon: 'error',

            title: 'Oops...',

            text: data.responseJSON.msg,

            customClass: 'swal-wide',

            })

        }

    });

}







    </script>
